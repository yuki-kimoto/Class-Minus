
SV *
name(self)
    SV *self
  ALIAS:
    name      = 0
    class     = 1
    is_common = 2
  CODE:
  {
    MethodMeta *meta = NUM2PTR(MethodMeta *, SvUV(SvRV(self)));
    switch(ix) {
      case 0: RETVAL = SvREFCNT_inc(meta->name); break;
      case 1:
        RETVAL = newSV(0);
        sv_setref_uv(RETVAL, "Class::Plain::MOP::Class", PTR2UV(meta->class));
        break;
      case 2: RETVAL = boolSV(meta->is_common); break;

      default: RETVAL = NULL;
    }
  }
  OUTPUT:
    RETVAL
